{
  "name": "nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "dev": "nodemon --files src/index.ts",
    "start": "node dist/index.js",
    "generate:prisma": "npx prisma migrate dev --name init",
    "start:dev": "npm run generate:prisma && npm run dev",
    "start:prod": "npm run generate:prisma && npm run test && npm start",
    "test": "jest -i",
    "test:watch": "jest -i --watchAll",
    "test:dev": "npm run generate:prisma && npm run test:watch"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/pg": "^8.10.1",
    "nodemon": "^2.0.22",
    "prisma": "^4.14.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "global": "^4.4.0",
    "pg": "^8.11.0"
  }
}
