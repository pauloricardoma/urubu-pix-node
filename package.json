{
  "name": "nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "start": "cross-env NODE_ENV=prod node dist/index.js",
    "dev": "cross-env NODE_ENV=test nodemon --files src/index.ts",
    "generate:prisma": "dotenv -e .env.prod -- npx prisma migrate dev",
    "generate:test": "dotenv -e .env.test -- npx prisma migrate dev",
    "test": "jest - i",
    "test:watch": "cross-env NODE_ENV=test jest -i --watchAll"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/pg": "^8.10.1",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.14.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "global": "^4.4.0",
    "pg": "^8.11.0"
  }
}
